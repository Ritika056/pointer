#include<stdio.h>
void swap(int*n1,int*n2)
{
      int temp;
      temp=*n1;
      *n1=*n2;
      *n2=temp;
}
int main()
{
      int a,b;
      printf("Enter Two Numbers: ");
      scanf("%d%d",&a,&b);
      printf("Numbers Before Swapping: ");
      printf("a=%d b=%d\n",a,b);
      swap(&a,&b);
      printf("Numbers After Swapping: ");
      printf("a=%d b=%d",a,b);
      return 0;
}
